pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'building World'
            }
        }
        stage('test') {
            steps {
                echo 'test World'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy World'
            }
        }
    }
}
